{
  "name": "AWS SageMaker Endpoints Resource Utilization",
  "description": null,
  "pages": [
    {
      "name": "AWS SageMaker Endpoints Resource Utilization",
      "description": null,
      "widgets": [
        {
          "title": "CPU/GPU Utilization by Endpoints",
          "layout": {
            "column": 1,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(`aws..aws.sagemaker.endpoints.CPUUtilization`) as 'CPU Utilization',  average(`aws..aws.sagemaker.endpoints.GPUUtilization`) as 'GPU Utilization'FROM Metric  TIMESERIES"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "title": "Memory/GPUMemory Utilization by Endpoints",
          "layout": {
            "column": 5,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(`aws..aws.sagemaker.endpoints.MemoryUtilization`) as 'Memory Utilization', average(`aws..aws.sagemaker.endpoints.GPUMemoryUtilization`) as 'GPU Memory Utilization'FROM Metric  TIMESERIES"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "title": "Disk Utilization by Endpoints",
          "layout": {
            "column": 9,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "visualization": {
            "id": "viz.stacked-bar"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(`aws..aws.sagemaker.endpoints.DiskUtilization`) as 'Average', max(`aws..aws.sagemaker.endpoints.DiskUtilization`) as 'Max',min(`aws..aws.sagemaker.endpoints.DiskUtilization`) as 'Min' FROM Metric  TIMESERIES"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            }
          }
        }
      ]
    }
  ]
}
